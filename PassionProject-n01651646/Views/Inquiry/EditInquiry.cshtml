@model PassionProject_n01651646.Models.InquiryDto

@{
    ViewBag.Title = "Edit Inquiry";
}

<h2>Edit Inquiry</h2>

<form method="POST" action="/Inquiry/Update/@Model.InquiryId">
    <input type="hidden" name="InquiryId" value="@Model.InquiryId" />
    <div>
        <label>Pet Name</label>
        <input type="text" name="PetName" value="@Model.PetName" />
    </div>
    <div>
        <label>Username</label>
        <input type="text" name="Username" value="@Model.Username" />
    </div>
    <div>
        <label>Inquiry Text</label>
        <textarea name="InquiryText">@Model.InquiryText</textarea>
    </div>
    <div>
        <input type="submit" value="Update" />
    </div>
</form>
